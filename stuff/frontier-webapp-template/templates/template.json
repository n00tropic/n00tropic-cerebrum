  "structure": [
    {
      "path": "apps/web/public/manifest.webmanifest",
      "type": "file"
    },
    {
      "path": "apps/web/public/robots.txt",
      "type": "file"
    },
    {
      "path": "apps/web/public/sitemap.xml",
      "type": "file"
    },
    {
